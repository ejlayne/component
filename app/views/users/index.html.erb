<h1>Hello Users!</h1>

<h1>Users</h1>
 
<table>
  <tr>
    <th>Username</th>
    <th>Email</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.username, user_path(user) %></td>
      <td><%= user.email %></td>
    </tr>
  <% end %>
</table>

<%= link_to "New User", new_user_path %>